{
  "extends": "expo/tsconfig.base",
  "compilerOptions": {
    "strict": true,
    "baseUrl": "./",
    "paths": {
      "@screens/*": ["src/screens/*"],
      "@navigation/*": ["src/navigation/*"],
      "@components/*": ["src/components/*"],
      "@theme/*": ["src/theme/*"]
    }
  },
  "include": ["./src", "App.tsx"],
  "exclude": ["node_modules", "babel.config.js"]
}
